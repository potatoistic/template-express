{
  "name": "express-nodejs-template",
  "version": "1.0.0",
  "description": "Template for Node.js and express projects.",
  "main": "main.js",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/main.js",
    "build:prod": "babel src --out-dir dist",
    "build": "babel src --out-dir temp",
    "lint": "eslint -f unix \"src/**/*.{js, jsx}\"",
    "test": "mocha __test__",
    "test:coverage": "nyc mocha __test__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhruvsaxena1998/express-nodejs-template.git"
  },
  "keywords": [
    "template",
    "nodejs",
    "express"
  ],
  "author": "dhruvsaxena1998 <saxenadhruv1927@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhruvsaxena1998/express-nodejs-template/issues"
  },
  "homepage": "https://github.com/dhruvsaxena1998/express-nodejs-template#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0"
  }
}
